          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://host.docker.internal:3001/refund</camunda:inputParameter>
            <camunda:inputParameter name="method">POST</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:inputParameter name="payload">
              <camunda:script scriptFormat="JavaScript">var payload = {
  "orderId": execution.getVariable("orderId"),
  "refundAmount": execution.getVariable("refundAmount")
};
JSON.stringify(payload);</camunda:script>
            </camunda:inputParameter>
          </camunda:inputOutput>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0xpxq8m</bpmn:incoming>
      <bpmn:outgoing>Flow_1c1v5m4</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="Event_ProcessFinished" name="Proces završen">
      <bpmn:incoming>Flow_1c1v5m4</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0yv5265" sourceRef="StartEvent_ReturnRequestReceived" targetRef="Activity_ValidateRequest" />
    <bpmn:sequenceFlow id="Flow_0k6a78i" sourceRef="Activity_ValidateRequest" targetRef="Gateway_RequestValid" />
    <bpmn:sequenceFlow id="Flow_Approved" name="Da" sourceRef="Gateway_RequestValid" targetRef="Activity_ApproveReturn">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isApproved == true}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Rejected" name="Ne" sourceRef="Gateway_RequestValid" targetRef="Activity_NotifyRejection">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${isApproved == false}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_1u3i2u1" sourceRef="Activity_NotifyRejection" targetRef="Event_ReturnRejected" />
    <bpmn:sequenceFlow id="Flow_0f612wn" sourceRef="Activity_ApproveReturn" targetRef="Event_PackageReceived" />
    <bpmn:sequenceFlow id="Flow_1s2qw81" sourceRef="Event_PackageReceived" targetRef="Activity_InspectItem" />
    <bpmn:sequenceFlow id="Flow_0xpxq8m" sourceRef="Activity_InspectItem" targetRef="Activity_ProcessRefund" />
    <bpmn:sequenceFlow id="Flow_1c1v5m4" sourceRef="Activity_ProcessRefund" targetRef="Event_ProcessFinished" />
