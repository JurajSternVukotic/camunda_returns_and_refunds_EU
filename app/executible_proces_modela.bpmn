    </bpmn:startEvent>
    <bpmn:serviceTask id="Activity_ValidateRequest" name="Validacija zahtjeva (API)">
      <bpmn:extensionElements>
        <camunda:connector>
          <camunda:connectorId>http-connector</camunda:connectorId>
          <camunda:inputOutput>
            <camunda:inputParameter name="url">http://host.docker.internal:3000/validate</camunda:inputParameter>
            <camunda:inputParameter name="method">POST</camunda:inputParameter>
            <camunda:inputParameter name="headers">
              <camunda:map>
                <camunda:entry key="Content-Type">application/json</camunda:entry>
              </camunda:map>
            </camunda:inputParameter>
            <camunda:inputParameter name="payload">
              <camunda:script scriptFormat="JavaScript">var payload = {
  "orderId": execution.getVariable("orderId"),
  "purchaseDate": execution.getVariable("purchaseDate")
};
JSON.stringify(payload);</camunda:script>
            </camunda:inputParameter>
            <camunda:outputParameter name="isApproved">
              <camunda:script scriptFormat="JavaScript">var response = S(response);
response.jsonPath('$.approved').boolValue();</camunda:script>
            </camunda:outputParameter>
          </camunda:inputOutput>
        </camunda:connector>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0yv5265</bpmn:incoming>
      <bpmn:outgoing>Flow_0k6a78i</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_RequestValid" name="Zahtjev ispravan?">
