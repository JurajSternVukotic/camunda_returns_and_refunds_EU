      <bpmn:outgoing>Flow_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:sendTask id="Activity_NotifyRejection" name="Obavijesti kupca o odbijanju" camunda:type="external" camunda:topic="notify-rejection">
